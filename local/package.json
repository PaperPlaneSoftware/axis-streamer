    "tailwindcss": "^3.0.23",
    "ts-pattern": "^4.0.1",
    "vee-validate": "^4.5.11",
    "vue-router": "^4.0.14",
    "ws": "^8.5.0",
    "yup": "^0.32.11"
  }
